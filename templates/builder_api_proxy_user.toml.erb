log_level="info"
enable_builder = false
app_url = "<%= node['builder']['oauth']['builder_url'] %>"

[oauth]
provider = "<%= node['builder']['oauth']['provider'] %>"
client_id = "<%= node['builder']['oauth']['client_id'] %>"
authorize_url = "<%= node['builder']['oauth']['authorize_url'] %>"
redirect_url = "<%= node['builder']['oauth']['redirect_url'] %>"

[nginx]
max_body_size = "2048m"
proxy_send_timeout = 180
proxy_read_timeout = 180
enable_gzip = true
enable_caching = true

[http]
keepalive_timeout = "180s"

[server]
listen_port = <%=  node['builder']['svc']['builder_api_proxy']['http_port'] %>
listen_tls = false

[analytics]
enabled = false
company_id = ""
company_name = ""
write_key = ""
